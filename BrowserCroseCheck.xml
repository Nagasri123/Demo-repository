<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="FreeCRM Suite" verbose="2">

	<test thread-count="5" name="Browser cross check with chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="pages.Login" />
		</classes>
	</test>
	<test thread-count="5" name="Browser cross check  with edge">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="pages.Login" />
		</classes>
	</test>
	<test   name="Browser cross check  firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="pages.Login" />
		</classes>
	</test>
</suite>  
