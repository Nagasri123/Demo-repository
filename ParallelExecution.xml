<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="FreeCRM Suite" verbose="2">

<!--for executing this remove @ignore in the package-info.java-->

 
	<test thread-count="5" name="executing with chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="pages.Login" />
			<class name="pages.Sample" />
		</classes>
	</test>

	<test thread-count="5" name="executing with edge">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="pages.Login" />
			<class name="pages.Sample" />
			<class name="pages.Sample2" />
		</classes>
	</test>

	<test thread-count="5" name="executing with chrome"  >
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="pages.Login" />
			<class name="pages.Sample" />
			<class name="pages.Sample2" />
			<class name="com.hyr.test.testFB" />
		</classes>
	</test>
</suite>  
